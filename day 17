import java.io.*;
import java.util.*;

class a {

 public void walk() 
  {
    System.out.println("I am walking");
  }
}
class brf extends a 
{
public void fly()
 {
    System.out.println("I am flying");
 }

public void sing()
    {
      System.out.println("I am singing");  
    }
 }
 public class dr
 {
     public static void main(String[] args) {
     brf r = new brf();
     r.walk();
     r.fly();
     r.sing();
    }
 }
