/* Java Output Formatting */

import java.util.Scanner;


public class Solution {

public static void main(String[] args) {
    
    Scanner sc=new Scanner(System.in);

    System.out.println("================================");
    System.out.printf("%-14s %03d\n",sc.next(),sc.nextInt());
    System.out.printf("%-14s %03d\n",sc.next(),sc.nextInt());
    System.out.printf("%-14s %03d\n",sc.next(),sc.nextInt());
    System.out.println("================================");
}
}




