import java.util.*;
class Solution
{
    public static void main(String args[])
    {
        int s=0;
        Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        for (int i = 1; i <= 10; i++)
        {
            s=n*i;
            System.out.println(n + " x " + i + " = " + s);
        }
    }
}
